= form_for Congressman.new do |f|
  = f.hidden_field :committee_id, value: committee.id
  .form-group
    = f.label :name
    = f.text_field :name, class: 'form-control'

  .form-group
    = f.label :description
    = f.text_field :description, class: 'form-control'

  .form-group
    = f.label :party
    = f.text_field :party, class: 'form-control'

  .form-group
    = f.submit class: 'btn btn-defult'
